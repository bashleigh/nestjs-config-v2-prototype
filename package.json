{
  "name": "nestjs-config-v2-test",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@nestjs/common": "^5.7.3",
    "@nestjs/core": "^5.7.3",
    "@nestjs/testing": "^5.7.3",
    "@types/dotenv": "^6.1.0",
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.9",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
  },
  "peerDependencies": {
    "@nestjs/common": "^5.7.3",
    "@nestjs/core": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage"
  },
  "dependencies": {
    "dotenv": "^6.2.0",
    "glob": "^7.1.3",
    "lodash.get": "^4.4.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.4.0"
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "prettier": "prettier **/**/*.ts --ignore-path ./.prettierignore --write && git status"
  }
}
